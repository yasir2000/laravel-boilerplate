# CrewAI Agent System Environment Configuration

# AI Model Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Default AI Model Settings
DEFAULT_LLM_MODEL=gpt-4-turbo-preview
DEFAULT_EMBEDDING_MODEL=text-embedding-3-small
TEMPERATURE=0.1
MAX_TOKENS=4000

# Database Configuration (should match Laravel .env)
DB_CONNECTION=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=laravel_hr_boilerplate
DB_USERNAME=postgres
DB_PASSWORD=your_db_password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# Laravel Application Integration
LARAVEL_API_URL=http://localhost:8000
LARAVEL_API_TOKEN=your_laravel_api_token
LARAVEL_APP_KEY=your_laravel_app_key

# Agent System Configuration
AGENT_MEMORY_BACKEND=redis
AGENT_LOG_LEVEL=INFO
AGENT_MAX_ITERATIONS=10
AGENT_EXECUTION_TIMEOUT=300

# External Service APIs
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_email_password

# File Storage Configuration
STORAGE_DRIVER=local
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=your_s3_bucket

# Monitoring and Logging
PROMETHEUS_PORT=9090
LOG_FILE_PATH=./logs/agents.log
ENABLE_METRICS=true
ENABLE_TRACING=true

# Development Settings
DEBUG=true
TESTING=false
ENVIRONMENT=development